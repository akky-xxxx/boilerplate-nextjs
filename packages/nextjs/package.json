{
  "name": "nextjs",
  "scripts": {
    "build": "next build",
    "build:storybook": "build-storybook",
    "check-all": "npm-run-all fix -p spell-check lint:css type-check -s test",
    "check-code": "run-p spell-check lint type-check",
    "dev": "next dev",
    "fix": "run-p fix:*",
    "fix-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "fix-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-eslint fix-ts-by-prettier",
    "hygen:ac": "hygen new atoms-component",
    "hygen:pc": "hygen new pages-component",
    "lint": "run-p lint:*",
    "lint-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --check",
    "lint:configs": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --check",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\"",
    "start": "next start",
    "storybook": "start-storybook -p 3050",
    "test": "jest",
    "type-check": "tsc"
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "axios": "^0.24.0",
    "next": "12.0.8",
    "pino": "^7.6.3",
    "pino-pretty": "^7.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-viewport": "^6.4.13",
    "@storybook/react": "^6.4.13",
    "@storybook/testing-library": "^0.0.7",
    "@storybook/testing-react": "^1.2.3",
    "@swc/core": "^1.2.129",
    "@swc/jest": "^0.2.17",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.8",
    "@types/react": "^17.0.38",
    "@types/styled-components": "^5.1.20",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-jest": "^27.4.6",
    "cspell": "^5.15.2",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "fixpack": "^4.0.0",
    "hygen": "^6.1.0",
    "jest": "^27.4.7",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "reg-keygen-git-hash-plugin": "^0.11.1",
    "reg-suit": "^0.11.1",
    "shx": "^0.3.4",
    "storycap": "^3.1.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-unused-exports": "^7.0.3",
    "typescript": "^4.5.4"
  },
  "private": true
}
