{
  "name": "nextjs",
  "scripts": {
    "api-gen": "run-p api-gen:*",
    "api-gen:strapi": "run-s apiClient:rm:strapi apiClient:gen:strapi",
    "apiClient:gen:strapi": "openapi2aspida -c \"./spec/openapi2aspida-config/strapi.js\"",
    "apiClient:rm:strapi": "shx rm -rf \"./src/libs/apiClient/strapi\"",
    "build": "next build",
    "build:storybook": "build-storybook",
    "check-all": "npm-run-all fix -p spell-check lint:css type-check -s test",
    "check-code": "run-p spell-check lint type-check",
    "compare-story": "run-s -c ss:capture reg-suit:save reg-suit:open",
    "dev": "nodemon",
    "fix": "run-p fix:*",
    "fix-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "fix-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-eslint fix-ts-by-prettier",
    "hygen:ac": "hygen new atoms-component",
    "hygen:mc": "hygen new molecules-component",
    "hygen:pc": "hygen new pages-component",
    "hygen:tc": "hygen new templates-component",
    "lint": "run-p lint:*",
    "lint-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --check",
    "lint:configs": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --check",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "reg-suit:open": "open-cli \"./report.html\"",
    "reg-suit:save": "reg-cli \"./.screenshot/actual\" \"./.screenshot/expected\" \"./.screenshot/diff\" -R \"./report.html\"",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\"",
    "ss:capture": "storycap http://localhost:3050 -o \".screenshot/actual\"",
    "ss:copy": "shx cp -r \"./.screenshot/actual/*\" \"./.screenshot/expected\"",
    "ss:git-add": "git add \"./.screenshot/expected\"",
    "ss:git-commit": "git commit -m \"docs(nextjs): update expected screenshot@`date \"+%Y-%m-%d %H:%M:%S\"`\"",
    "ss:remove": "shx rm -rf \"./.screenshot/expected/*\"",
    "start": "NODE_ENV=production nodemon",
    "storybook": "start-storybook -p 3050",
    "test": "run-s test:*",
    "test:client": "jest",
    "test:server": "jest -c jest.server.config.js",
    "type-check": "tsc",
    "update-ss": "run-s ss:remove ss:copy ss:git-add ss:git-commit"
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "axios": "^0.24.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3",
    "winston": "^3.5.1"
  },
  "devDependencies": {
    "@stoplight/prism-cli": "^4.7.0",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-interactions": "^6.4.18",
    "@storybook/addon-viewport": "^6.4.13",
    "@storybook/jest": "^0.0.6",
    "@storybook/react": "^6.4.13",
    "@storybook/testing-library": "^0.0.7",
    "@storybook/testing-react": "^1.2.3",
    "@swc/core": "^1.2.129",
    "@swc/jest": "^0.2.17",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.8",
    "@types/react": "^17.0.38",
    "@types/styled-components": "^5.1.20",
    "@types/supertest": "^2.0.11",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-jest": "^27.4.6",
    "cspell": "^5.15.2",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "fixpack": "^4.0.0",
    "hygen": "^6.1.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "openapi2aspida": "^0.17.0",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "reg-keygen-git-hash-plugin": "^0.11.1",
    "reg-suit": "^0.11.1",
    "shx": "^0.3.4",
    "storybook-addon-next-router": "^3.1.1",
    "storybook-addon-pseudo-states": "^1.7.0",
    "storycap": "^3.1.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "supertest": "^6.2.2",
    "ts-node": "^10.4.0",
    "ts-unused-exports": "^7.0.3",
    "typescript": "^4.5.4"
  },
  "version": "0.1.0",
  "private": true
}
